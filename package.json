{
  "name": "vue3-template",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --config ./build/vite.config.dev.ts",
    "build": "vue-tsc --noEmit && vite build --config ./build/vite.config.prod.ts",
    "preview": "yarn build && vite preview --host",
    "report": "cross-env REPORT=true npm run build",
    "type:check": "vue-tsc --noEmit --skipLibCheck",
    "lint:lint-staged": "lint-staged",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "prepare": "husky install",
    "upgrade-interactive": "yarn upgrade-interactive --latest"
  },
  "dependencies": {
    "@arco-design/web-vue": "^2.37.3",
    "@arco-themes/vue-hospital-system": "^0.0.3",
    "@vee-validate/i18n": "^4.6.10",
    "@vee-validate/rules": "^4.6.10",
    "@vee-validate/zod": "^4.6.10",
    "@vueuse/core": "^9.3.0",
    "@vueuse/integrations": "^9.3.0",
    "axios": "^0.27.2",
    "axios-auth-refresh": "^3.3.3",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.5",
    "echarts": "^5.4.0",
    "localforage": "^1.10.0",
    "lodash-es": "^4.17.21",
    "mitt": "^3.0.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.22",
    "query-string": "^7.1.1",
    "uuid": "^9.0.0",
    "vee-validate": "^4.6.10",
    "vite-plugin-banner": "^0.5.0",
    "vue": "^3.2.40",
    "vue-echarts": "^6.2.3",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.5",
    "vue-types": "^4.2.1",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@arco-plugins/vite-vue": "^1.3.8",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@julr/vite-plugin-validate-env": "^0.1.3",
    "@types/crypto-js": "^4.1.1",
    "@types/lodash": "^4.14.186",
    "@types/lodash-es": "^4.17.6",
    "@types/mockjs": "^1.0.7",
    "@types/node": "^18.7.23",
    "@types/nprogress": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "@vitejs/plugin-legacy": "^2.2.0",
    "@vitejs/plugin-vue": "^3.1.0",
    "@vitejs/plugin-vue-jsx": "^2.0.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.1.3",
    "consola": "^2.15.3",
    "conventional-changelog-cli": "^2.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.24.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.5.1",
    "husky": "^8.0.1",
    "less": "^4.1.2",
    "lint-staged": "^13.0.3",
    "mockjs": "^1.1.0",
    "normalize.css": "^8.0.1",
    "postcss": "^8.4.16",
    "postcss-html": "^1.5.0",
    "postcss-less": "^6.0.0",
    "prettier": "^2.2.1",
    "rollup-plugin-visualizer": "^5.8.2",
    "stylelint": "^14.13.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended": "^9.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^28.0.0",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.8.4",
    "unplugin-auto-import": "^0.11.2",
    "unplugin-vue-components": "^0.22.7",
    "vite": "^3.1.4",
    "vite-plugin-cdn-import": "^0.3.5",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-eslint": "^1.3.0",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-pwa": "^0.13.1",
    "vite-plugin-style-import": "2.0.0",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vite-plugin-windicss": "^1.8.8",
    "vite-svg-loader": "^3.1.0",
    "vue-tsc": "^0.40.13",
    "windicss": "^3.5.6"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "resolutions": {
    "bin-wrapper": "npm:bin-wrapper-china",
    "rollup": "^2.56.3",
    "gifsicle": "5.2.0",
    "conventional-changelog-cli": "^2.2.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
      "prettier --write--parser json"
    ],
    "package.json": [
      "prettier --write"
    ],
    "*.vue": [
      "eslint --fix",
      "prettier --write",
      "stylelint --fix"
    ],
    "*.{scss,less,styl,html,css}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  }
}
